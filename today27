#include <stdio.h>

int main()
{
    //printf("Hello World");
    int n,i,j,k=0,l=0,min,a[100],b[100],c[100],count;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            count=1;
            for(j=i+1;j<n;j++)
            {
                if(a[j]%2==0)
                {
                    count++;
                }
            }
        }
    }

    return 0;
}
